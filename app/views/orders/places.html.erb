<div class="container-fluid">
  <div class="row row-fluid">
    <div class="col-md-2">
      <%= render "share/menu_admin" %>
    </div>
    <div class="col-md-10">
      <%= render "orders/links" %>
      <% if @places.present? %>
        <table class="table">
          <tr>
            <th>#</th>
            <th><%= t ".name" %></th>
            <th><%= t ".email" %></th>
            <th><%= t ".address" %></th>
            <th><%= t ".phone" %></th>
            <th><%= t ".status" %></th>
            <th><%= t ".next" %></th>
          </tr>
          <%= render @places %>
        </table>
        <%= will_paginate @places %>
      <% end %>
    </div>
  </div>
</div>
