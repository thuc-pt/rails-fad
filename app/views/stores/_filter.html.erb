<%= search_form_for object, url: url do |f| %>
  <div class="form-group">
    <%= f.search_field :name_cont, placeholder: t(".name"), class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.text_field :price_gteq, placeholder: t(".min"), class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.text_field :price_lteq, placeholder: t(".max"), class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.select :category_id_eq, load_category, {prompt: t(".category")},
      class: "form-control" %>
  </div>
  <div class="form-group text-center">
    <%= f.submit t(".submit"), class: "btn btn-success" %>
  </div>
<% end %>
